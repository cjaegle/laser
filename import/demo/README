DB Setup

create database KBPlus default character set utf8 default collate utf8_bin;
grant all on KBPlus.* to 'k-int'@'localhost';
grant all on KBPlus.* to 'k-int'@'localhost.localdomain';
grant all on KBPlus.* to 'k-int'@'%';


