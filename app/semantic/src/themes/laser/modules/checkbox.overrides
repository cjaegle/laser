/*******************************
         Theme Overrides
*******************************/

@font-face {
  font-family: 'Checkbox';
  src:
          url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype')
;
}

/* Checkmark */
.ui.checkbox label:after,
.ui.checkbox .box:after {
  font-family: 'Checkbox';
}

/* Checked */
.ui.checkbox input:checked ~ .box:after,
.ui.checkbox input:checked ~ label:after {
  content: '\e800';
  color: @grey;
  background-color: @laFilterChosen !important;
}

/* Indeterminate */
.ui.checkbox input:indeterminate ~ .box:after,
.ui.checkbox input:indeterminate ~ label:after {
  font-size: 12px;
  content: '\e801';
}


/*  UTF Reference
.check:before { content: '\e800'; }
.dash:before  { content: '\e801'; }
.plus:before { content: '\e802'; }
*/

/************* laser three toggle icon buttons ***************/

.radio, .checkbox.la-toggle-radio {
  padding:0;
}
/* BEFORE */
.ui.checkbox.la-toggle-radio label{
  font-size: 1.7em;
  width: 40px;
  height: 40px;
  color: @textColor;
}

.ui.checkbox.la-toggle-radio label:before {

  //border: none;
  //font-size: 25px;

  border: none;
  width: 40px;
  height: 40px;
  padding-top: 11px;
  text-align: center;
  background-color:@backgroundColor;
  border-radius: 500rem;
}
.ui.checkbox.la-toggle-radio input:checked ~ label:before {
  background-color:@backgroundColor;
}
/* AFTER */
.ui.checkbox.la-toggle-radio input:checked~label:after,
.ui.radio.checkbox.la-toggle-radio input:checked~label:after {
  color: @green !important;
  top: 25px !important;
  left: 25px !important;
  background-color: white !important;
  border-radius: 500rem;
  box-shadow: 0 2px 3px 0 #d3dae3;
 /*
  content: "\f00c" !important;
  font-family: Icons !important;
  transform: scale(1.5);
  */
}
/* APPEND */
.ui.checkbox.la-toggle-radio.la-append label:before {
  font-family: icomoon;
  content: "\e90c";

}
/* REPLACE */
.ui.checkbox.la-toggle-radio.la-replace label:before {
  font-family: icomoon;
  content: "\e90d";
}
/* NO CHANGE */
.ui.checkbox.la-toggle-radio.la-noChange label:before {
  font-family: Icons;
  content: "\f05e";
}

 table tr td div.willStay {
   background-color: #D4E096;
 }
table tr td div.willBeReplaced {
  background-color: #EDC0BA;
  text-decoration: line-through;
}
.la-test {
  padding: 3px 0;
}